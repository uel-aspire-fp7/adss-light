<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.commands">
      <category
            description="ASPIRE WP4"
            id="org.uel.aspire.wp4.assessment.commands.category"
            name="UEL Assessment">
      </category>
      <command
            category="org.uel.aspire.wp4.assessment.commands.category"
            categoryId="org.uel.aspire.wp4.assessment.commands.category"
            id="org.uel.aspire.wp4.assessment.commands.RunSimulation"
            name="PN Simulation">
      </command>
      <command
            category="org.uel.aspire.wp4.assessment.commands.category"
            categoryId="org.uel.aspire.wp4.assessment.commands.category"
            id="org.uel.aspire.wp4.assessment.commands.PNAssessment"
            name="Petri Net Assessment">
      </command>
      <command
            category="org.uel.aspire.wp4.assessment.commands.category"
            categoryId="org.uel.aspire.wp4.assessment.commands.category"
            id="org.uel.aspire.wp4.assessment.commands.fitnessassessment"
            name="FitnessAssessment">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.uel.aspire.wp4.assessment.handlers.RunSimulationHandler"
            commandId="org.uel.aspire.wp4.assessment.commands.RunSimulation">
      </handler>
      <handler
            class="org.uel.aspire.wp4.assessment.handlers.PetriNetAssessment"
            commandId="org.uel.aspire.wp4.assessment.commands.PNAssessment">
      </handler>
      <handler
            class="org.uel.aspire.wp4.assessment.handlers.FitnessAssessmentHandler"
            commandId="org.uel.aspire.wp4.assessment.commands.fitnessassessment">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
         <menu
               icon="icons/sample.gif"
               id="org.uel.aspire.wp4.assessment.menus.UEL-ASPIREMenu"
               label="UEL-ASPIRE"
               mnemonic="U">
            <command
                  commandId="org.uel.aspire.wp4.assessment.commands.fitnessassessment"
                  icon="icons/sample.gif"
                  label="Fitness Function Assessment"
                  style="push">
            </command>
            <command
                  commandId="org.uel.aspire.wp4.assessment.commands.RunSimulation"
                  icon="icons/sample.gif"
                  id="org.uel.aspire.wp4.assessment.RunSimulation"
                  label="Atttack Simulation">
            </command>
            <command
                  commandId="org.uel.aspire.wp4.assessment.light.command1"
                  icon="icons/sample.gif"
                  label="Fitness Function Simulation"
                  style="push">
            </command>
         </menu>
         
         
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="org.uel.aspire.wp4.assessment.toolbars.sampleToolbar1"
               label="Fitness Function Assessment">
         </toolbar>
         <toolbar
               id="org.uel.aspire.wp4.assessment.light.toolbar1"
               label="Fitness Function Simulation">
         </toolbar>
         <toolbar
               id="org.uel.aspire.wp4.assessment.toolbars.sampleToolbar"
               label="Attack Simulation">
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.uel.aspire.wp4.assessment.UELView"
            name="UELView">
      </category>
      <view
            allowMultiple="false"
            category="org.uel.aspire.wp4.assessment.UELView"
            class="org.uel.aspire.wp4.assessment.views.UELView"
            icon="icons/sample.gif"
            id="org.uel.aspire.wp4.assessment.views.UELView"
            name="ASPIRE Assessment "
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="org.uel.aspire.wp4.assessment.UELView"
            class="org.uel.aspire.wp4.assessment.views.UELPSView"
            icon="icons/sample.gif"
            id="org.uel.aspire.wp4.assessment.views.UELPSView"
            name="UELPSView"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="org.uel.aspire.wp4.assessment.UELView"
            class="org.uel.aspire.wp4.assessment.views.UELMetricView"
            icon="icons/sample.gif"
            id="org.uel.aspire.wp4.assessment.views.UELMetricView"
            name="UELMetricView"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="org.uel.aspire.wp4.assessment.UELView"
            class="org.uel.aspire.wp4.assessment.views.UELEditView"
            icon="icons/sample.gif"
            id="org.uel.aspire.wp4.assessment.views.UELEditView"
            name="UELEditView"
            restorable="true">
      </view>
   </extension>

</plugin>
